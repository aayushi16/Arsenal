<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>


<class table="userdetails_mst" name="VO.UserDetailsVO" lazy="false">


<id name="user1_id" column="ID" type="int">

<generator class="increment"/>

</id>
<property name="user1_no">

<column name="user1_no"/>

</property>


<property name="first_name">

<column name="first_name"/>

</property>


<property name="last_name">

<column name="last_name"/>

</property>

<property name="address1">

<column name="address1"/>

</property>
<property name="address2">

<column name="address2"/>

</property>
<property name="contact_no">

<column name="contact_no"/>

</property>
<property name="email_id">

<column name="email_id"/>

</property>
<property name="post">

<column name="post"/>

</property>
<property name="gun1_no">

<column name="gun1_no"/>

</property>
<many-to-one name="userVO" class="VO.UserVO" column="UserId"></many-to-one>

</class>

</hibernate-mapping>
